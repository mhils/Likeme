{
  "manifest_version" : 2,
  "name": "__MSG_appName__",
  "description": "__MSG_appDesc__",
  "version": "1.1.1", 
  "default_locale": "en",
  "icons": { "16" : "img/like-16.png",
             "48" : "img/like-48.png",
			 "128": "img/like-128.png",
             "256": "img/like-256.png" },
  "background": { "scripts": ["background.js"] },
  "page_action": {
     "default_icon": "img/fb.png",
     "default_title": "__MSG_pageaction_allow_once__"
  },
  "web_accessible_resources": [
	"facebook/like.html",
	"facebook/fb_fake.js",
	"facebook/fb_load.js"
  ],
  "permissions": ["*://*/*","webRequest","webRequestBlocking","tabs"]
}
